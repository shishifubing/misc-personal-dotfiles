{
    "telemetry.enableTelemetry": false,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "window.openFilesInNewWindow": "on",
    "window.dialogStyle": "native",
    "editor.tabSize": 4,
    "explorer.openEditors.visible": 999,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.quickSuggestionsDelay": 400,
    "editor.minimap.enabled": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.fontSize": 25,
    "editor.detectIndentation": false,
    "editor.accessibilitySupport": "off",
    "editor.suggestSelection": "first",
    "editor.formatOnPaste": true,
    "workbench.editor.showTabs": false,
    "workbench.colorTheme": "Default High Contrast",
    "workbench.colorCustomizations": {
        "contrastBorder": "#9932cc00",
        "editor.selectionHighlightBorder": "#9932cc",
        "focusBorder": "#9932cc",
        "statusBar.border": "#9932cc00",
        "statusBar.debuggingBackground": "#9932cc80",
        "editor.lineHighlightBorder": "#9932cc",
        "editor.selectionBackground": "#9932cc",
        "editor.selectionForeground": "#ffffff",
    },
    "workbench.statusBar.visible": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "workbench.panel.opensMaximized": "always",
    "zenMode.centerLayout": false,
    "zenMode.silentNotifications": false,
    "workbench.editor.decorations.badges": true,
    "workbench.editor.labelFormat": "long",
    "workbench.editor.limit.value": 999,
    "explorer.incrementalNaming": "smart",
    "window.zoomLevel": -1,
    "workbench.editor.enablePreview": false,
    "editor.cursorSmoothCaretAnimation": true,
    "workbench.startupEditor": "newUntitledFile"
}