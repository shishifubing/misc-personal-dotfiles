{
  "manifest_version": 2,
  "name": "json-button",
  "version": "0.14",
  "description": "new tab bookmarks",
  "icons": {
    "48": "icons/black-square48.png"
  },
  "background": {
    "scripts": ["js/main.js"]
  },
